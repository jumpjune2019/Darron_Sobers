package assignment6;

public class LabellingPairOfDices {

	public static void main(String[] args) {
		int colDie = 0, rowDie = 0;
		String[][] matrix = new String[6][6];
		
		for(int x = 0; x <matrix.length; x++) {
			System.out.print("$");
			for(int y = 0; y <matrix[0].length; y++) {
				System.out.print("$");
				System.out.print(y + " ");
				
			}
			System.out.print("\n");
		}
	}

}
