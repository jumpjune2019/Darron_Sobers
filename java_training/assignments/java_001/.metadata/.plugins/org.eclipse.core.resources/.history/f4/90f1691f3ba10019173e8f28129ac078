package app;
import arrayutils.ArrUtil;
public class Main {

	public static void main(String[] args) {
		String[] myList = {"Bozo", "FooBar", "Delta", "Foozball", "Demo", "Money", "Zoo", "Zoo"};
		System.out.println("This is the a test of the arrayHasExactMatch and indexOfOccurenceInArray methods");
		System.out.println("The array to test contains the following items");
		System.out.println("{\"Bozo\", \"FooBar\", \"Delta\", \"Foozball\", \"Demo\", \"Money\", \"Zoo\", \"Zoo\"}");
		System.out.println("Scenario 1\n" + "arrayHasExactMatch (myList, \"zo\", false): is " + ArrUtil.arrayHasExactMatch(myList, "zo", false) + "\n");
		System.out.println("Scenario 2\n" + "arrayHasExactMatch (myList, \"zoo\", false): is " + ArrayStringUtilitiesPart2.arrayHasExactMatch(myList, "zoo", false) + "\n");
		System.out.println("Scenario 3\n" + "arrayHasExactMatch (myList, \"zoo\", true): is " + ArrayStringUtilitiesPart2.arrayHasExactMatch(myList, "zoo", true) + "\n");
		System.out.println("Scenario 4\n" + "arrayHasExactMatch (myList, \"foo\", true): is " + ArrayStringUtilitiesPart2.arrayHasExactMatch(myList, "foo", true) + "\n");
		System.out.println("Scenario 5\n" + "arrayHasExactMatch (myList, \"foo\", false): is " + ArrayStringUtilitiesPart2.arrayHasExactMatch(myList, "foo", false) + "\n");
		System.out.println("Scenario 6\n" + "arrayHasExactMatch (myList, \"delta\", true): is " + ArrayStringUtilitiesPart2.arrayHasExactMatch(myList, "delta", true) + "\n");
		System.out.println("Scenario 7\n" + "arrayHasExactMatch (myList, \"Delta\", true): is " + ArrayStringUtilitiesPart2.arrayHasExactMatch(myList, "Delta", true) + "\n");
		System.out.println("Second half of testing\n\n");
		System.out.println("Scenario 8\n" + "indexOfOccuranceInArray (myList, \"zo\", false): is " + "[" +indexOfOccuranceInArray(myList, "zo", false) + "]" + "\n");
		System.out.println("Scenario 9\n" + "indexOfOccuranceInArray (myList, \"zoo\", false): is " + "[" +indexOfOccuranceInArray(myList, "zoo", false) + "]" + "\n");
		System.out.println("Scenario 10\n" + "indexOfOccuranceInArray (myList, \"zoo\", true): is " + "[" +indexOfOccuranceInArray(myList, "zoo", true) + "]" + "\n");
		System.out.println("Scenario 11\n" + "indexOfOccuranceInArray (myList, \"foo\", true): is " + "[" +indexOfOccuranceInArray(myList, "foo", true) + "]" + "\n");
		System.out.println("Scenario 12\n" + "indexOfOccuranceInArray (myList, \"foo\", false): is " + "[" +indexOfOccuranceInArray(myList, "foo", false) + "]" + "\n");
		System.out.println("Scenario 13\n" + "indexOfOccuranceInArray (myList, \"delta\", true): is " + "[" +indexOfOccuranceInArray(myList, "delta", true) + "]" + "\n");
		System.out.println("Scenario 14\n" + "indexOfOccuranceInArray (myList, \"Delta\", true): is " + "[" +indexOfOccuranceInArray(myList, "Delta", true) + "]" + "\n");

	}

}
